src_compile() {
	export -n GOCACHE XDG_CACHE_HOME
	env GOBIN="${S}/bin" go install ./... || die
}

src_install() {
	einstalldocs
	doinitd "${FILESDIR}/init.d/rtsp-simple-server"
	insinto /etc
	doins rtsp-simple-server.yml
	dobin bin/*
}

src_test() {
	emake test-internal
}
